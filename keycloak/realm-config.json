{
    "realm": "clinic-hub",
    "enabled": true,
    "displayName": "Mouzaia Clinic Hub",
    "users": [
        {
            "username": "${KEYCLOAK_ADMIN}",
            "enabled": true,
            "firstName": "Clinic",
            "lastName": "Admin",
            "email": "${KEYCLOAK_ADMIN_EMAIL}",
            "credentials": [
                {
                    "type": "password",
                    "value": "${KEYCLOAK_ADMIN_PASSWORD}",
                    "temporary": false
                }
            ]
        }
    ],
    "clients": [
        {
            "clientId": "clinic-nextjs",
            "enabled": true,
            "redirectUris": [
                "${KEYCLOAK_REDIRECT_URI_LOCAL}",
                "${KEYCLOAK_REDIRECT_URI_APP}"
            ],
            "publicClient": true,
            "protocol": "openid-connect"
        }
    ]
}